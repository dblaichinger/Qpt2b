 <section id="content">
    

        
        	<h3 class="admin"> Vorhandene Mülleimer </h3>

        	<%if @trashcans != [] %>
				<table class="trashcan">
					<tr>
						<th></th>
						<th></th>
						<th>ID</th>
						<th>Adress</th>
						<th>Latitude</th>
						<th>Longitude</th>
						<th>Is_free</th>
						<th>Adopted_until</th>
						<th>Created_at</th>
						<th>Updated_at</th>
					</tr>
				
				
					<% @trashcans.each do |trashcan| %>
					<tr>
						<td class="icons"><%= link_to image_tag("/images/delete.png"), trashcan, :method => :delete, 
						 	:confirm => "Attention:\nAre you sure to delete trashcan?", :title => "Delete #{trashcan.address}"
							%>
						</td>
						<td><%= link_to image_tag("/images/edit.png"), edit_trashcan_path(trashcan) %></td>
						<td><%= trashcan.id %></td>
						<td><%= trashcan.address %></td>
						<td><%= trashcan.latitude %></td>
						<td><%= trashcan.longitude %></td>
						<td><%= trashcan.is_free %></td> 
						<td><%= trashcan.adopted_until %></td>
						<td><%= trashcan.created_at %></td>
						<td><%= trashcan.updated_at %></td>
					</tr>
					<% end %>
				</table>
			<% else %>
				<p>Noch keine Mülleimer vorhanden</p>
			<% end %>
			


	<h3 class="admin">Neue Mülleimer</h3>
<%if @demands != [] %>
	<table class="trashcan">
		<tr>
			<th></th>
			<th></th>
			<th>ID</th>
			<th>Adress</th>
			<th>Latitude</th>
			<th>Longitude</th>
			<th>Votes</th>
		</tr>
	

		<% @demands.each do |demand| %>
		<tr>
			<td class="icons"><%= link_to image_tag("../images/delete.png"), demand, :method => :delete, 
			 	:confirm => "Attention:\nAre you sure to delete this demand?", :title => "Delete #{demand.address}"
				%>
			</td>
			<td><%= link_to image_tag("../images/edit.png"), edit_demand_path(demand) %></td>
			<td><%= demand.id %></td>
			<td><%= demand.address %></td>
			<td><%= demand.latitude %></td>
			<td><%= demand.longitude %></td>
			<td><%= demand.counter %></td> 
		</tr>
		<% end %>
	</table>	
<% else %>
	<p>Noch keine Votings vorhanden</p>
<% end %>

</section>